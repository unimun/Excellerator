{
  "name": "srprj",
  "version": "1.0.0",
  "description": "srprj",
  "main": "dist/js/main/index.js",
  "scripts": {
    "copy:resources": "cpx \"prod_resources/**/*\" dist",
    "copy:photon": "cpx \"node_modules/photon/dist/css/**/*\" dist/css && cpx \"node_modules/photon/dist/fonts/**/*\" dist/fonts",
    "copy:7z": "cpx \"node_modules/7zip/7zip-lite/**/*\" dist/js/main/7zip-lite --verbose",
    "prebundle": "npm run copy:resources && npm run copy:photon",
    "bundle": "webpack",
    "prepackage": "npm run bundle",
    "package": "electron-packager dist \"Excellerator\" --all --out=release --icon=\"icon/icon\"",
    "watch": "webpack --watch",
    "start": "electron ."
  },
  "author": "dobby.han",
  "license": "ISC",
  "devDependencies": {
    "7zip": "0.0.6",
    "acorn": "^5.1.2",
    "asar": "^0.13.0",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-react": "^6.16.0",
    "bufferutil": "^3.0.2",
    "cpx": "^1.5.0",
    "css-loader": "^0.28.7",
    "electron": "^1.7.8",
    "electron-devtools-installer": "^2.2.0",
    "electron-packager": "^9.1.0",
    "electron-winstaller": "^2.6.3",
    "eslint": "^4.7.2",
    "postcss": "^6.0.11",
    "postcss-loader": "^2.0.6",
    "react-redux": "^5.0.6",
    "remote-redux-devtools": "^0.5.12",
    "spectron": "^3.6.0",
    "style-loader": "^0.13.1",
    "styled-components": "^2.1.2",
    "utf-8-validate": "^3.0.3",
    "webpack": "^2.2.0"
  },
  "dependencies": {
    "colormin": "^1.1.2",
    "cssnano": "^3.10.0",
    "immutable": "^3.8.1",
    "material-ui": "^1.0.0-beta.12",
    "material-ui-icons": "^1.0.0-beta.14",
    "photon": "github:connors/photon",
    "postcss-colormin": "^2.2.2",
    "postcss-convert-values": "^2.6.1",
    "postcss-discard-comments": "^2.0.4",
    "postcss-filter-plugins": "^2.0.2",
    "postcss-minify-gradients": "^1.0.5",
    "postcss-minify-selectors": "^2.1.1",
    "postcss-ordered-values": "^2.2.3",
    "postcss-reduce-initial": "^1.0.1",
    "postcss-reduce-transforms": "^1.0.4",
    "postcss-svgo": "^2.1.6",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-immutable-proptypes": "^2.1.0",
    "react-router": "^4.2.0",
    "react-swipeable-views": "^0.12.8",
    "react-tabs": "^2.0.0",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1"
  }
}
